<<%= link_to user_packed_bag_path(@packed_bag, params: { category: 0, owned: params[:owned] }), class: "", remote: :true do %>
  <%#= image_tag "categories/all" %>
  <small><strong>ALL</strong></small>
<% end %>

<% categories.each do |category| %>
  <%= link_to user_packed_bag_path(@packed_bag, params: { category: category.id, owned: params[:owned] }), class: "", remote: :true do %>
    <%#= image_tag "categories/#{category.picture}" %>
    <small><%= category.name.upcase %></small>
  <% end %>
<% end %>
