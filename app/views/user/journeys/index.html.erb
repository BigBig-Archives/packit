<h2>My journeys</h2>

<div class="container" id="journeys-container">
  <div class="row">
    <% current_user.journeys.each do |journey| %>
        <%= render 'user/journeys/journey',
          journey: journey
        %>
      <% end %>
  </div>
</div>

<hr>

<h2>Add a new journey</h2>

<%= render 'user/journeys/form',
  journey: @journey
%>
