<div id="references">
  <p class="cards-indication">Here are all the items you can create</p>
  <hr>
  <div class="items-container">
    <% @references.each do |reference| %>
      <%= render 'item_references/reference', reference: reference %>
    <% end %>
    <%= render 'user/packed_bags/cards/shadow_cards' %>
  </div>
</div>
