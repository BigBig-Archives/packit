function filter() {
  // Operation
  let operations_container = document.querySelector("#navbar #operations");
  let operations = `<%= j render 'user/packed_bags/navbar/operations' %>`;
  operations_container.innerHTML = operations;
  // Categories
  let categories_container = document.querySelector("#navbar #categories");
  let categories = `<%= j render 'user/packed_bags/navbar/categories' %>`;
  categories_container.innerHTML = categories;
  // Display
  let displays_container = document.querySelector("#navbar #displays");
  let displays = `<%= j render 'user/packed_bags/navbar/displays' %>`;
  displays_container.innerHTML = displays;
  // Items displayed
  let items_displayed_container = document.querySelector("#packed-bag-wrapper");
  let items_displayed = `<%= j render 'user/packed_bags/displayer' %>`;
  items_displayed_container.innerHTML = items_displayed;
}
filter();
